@model NewsSite.Models.Visitor

@{
    ViewBag.Title = "Registration";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.ViewTitle = Model.Id == 0 ? Html.LabelForModel() : new MvcHtmlString(string.Format("Editing user - {0} {1}", Model.FirstName, Model.LastName));
}

@Html.Partial("EditVisitorForm", Model)
